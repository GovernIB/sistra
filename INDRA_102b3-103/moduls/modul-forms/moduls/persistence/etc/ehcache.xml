<ehcache>

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
        maxElementsInMemory="2000"
        eternal="false"
        timeToIdleSeconds="600"
        timeToLiveSeconds="1200"
        overflowToDisk="false"
        />

    <cache name="net.sf.hibernate.cache.UpdateTimestampsCache"
        maxElementsInMemory="1000"
        eternal="true"
        overflowToDisk="false"/>

</ehcache>
