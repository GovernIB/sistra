<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<class name="es.caib.bantel.model.AvisosBandeja" table="BTE_AVISOS">
	<cache usage="read-write"/>
     <id name="identificador" unsaved-value="null" column="AVI_IDENT">
        <generator class="assigned" />
      </id>
	
	<property name="fechaUltimoAviso"  column="AVI_FCAVIS"  not-null="true"/>			
	
</class>
</hibernate-mapping>