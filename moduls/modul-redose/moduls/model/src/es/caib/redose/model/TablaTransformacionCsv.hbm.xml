<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="es.caib.redose.model.TablaTransformacionCsv" table="RDS_TABCSV">
    	<cache usage="read-write"/>
    	
        <id name="codigo" type="string" length="1" unsaved-value="null" column="TCS_CODIGO">
            <generator class="assigned"/>
        </id>
        
        <property name="claves" type="binary" column="TCS_CLAVES" not-null="true"/>
        
    </class>
</hibernate-mapping>
