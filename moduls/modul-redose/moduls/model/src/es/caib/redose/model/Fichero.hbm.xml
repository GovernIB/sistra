<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="es.caib.redose.model.Fichero" table="RDS_FICHER">
    	<cache usage="read-write"/>
    	    	
    	<id name="codigo" unsaved-value="null" column="FIC_CODIGO">
            <generator class="assigned"/>
        </id>

        <property name="datos" type="binary" column="FIC_DATOS" not-null="true" />
    </class>
</hibernate-mapping>
