<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="org.ibit.rol.form.model.Version" table="RFR_VERSIO">
        <cache usage="read-write"/>
        <id name="codigo" unsaved-value="null" column="VER_CODIGO">
	        <generator class="assigned" />
	      </id>

        <property name="nombre" column="VER_NOMBRE" />
        <property name="fecha" column="VER_FECHA" />
        <property name="sufijo" column="VER_SUFIX" />
        
    </class>

</hibernate-mapping>

