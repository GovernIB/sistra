<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

	<class name="es.caib.bantel.model.FicheroExportacion" lazy="true" proxy="es.caib.bantel.model.FicheroExportacion" table="BTE_FICEXP">
	<cache usage="read-write"/>
    <!--  
    <id name="codigo" unsaved-value="null" column="ARP_CODPLI">
        <generator class="assigned" />
    </id>
     -->
     
	<id name="codigo" column="FIC_IDETRA">
        <generator class="foreign">
            <param name="property">tramite</param>
        </generator>
    </id>

    <property name="datos" type="binary" column="FIC_DATOS" not-null="true"/>

    <!-- Associations -->
  
  
    <!-- bi-directional one-to-one association to RdsPlaidi -->
    <one-to-one name="tramite"
        class="es.caib.bantel.model.Tramite"
        outer-join="false"
        constrained="true"/>


</class>
</hibernate-mapping>
