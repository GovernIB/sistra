<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="es.caib.redose.model.RdsDoc" table="RDS_DOC">
        <!--  <cache usage="read-write"/> -->
        <id name="id" unsaved-value="null" column="ID">
            <generator class="sequence">
                <param name="sequence">DUMMY_SEQ</param>
            </generator>
        </id>
        
        <property name="nombre" not-null="true" column="NOMBRE"/>
        <property name="blob_Documento" not-null="false" column="BLOB_DOCUMENTO" type="es.caib.redose.type.BLOBType"/>
    </class>
</hibernate-mapping>