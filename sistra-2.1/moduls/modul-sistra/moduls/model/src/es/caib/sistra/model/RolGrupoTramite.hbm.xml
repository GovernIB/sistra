<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="es.caib.sistra.model.RolGrupoTramite" table="STR_GRPTRA">
        <cache usage="read-write"/>
       	<composite-id name="id" class="es.caib.sistra.model.RolGrupoTramiteId" >
            <key-property name="codiGrup" type="string">
                <column name="GRT_CODGRP" />
            </key-property>
            <key-property name="codiTra" type="java.lang.Long">
                <column name="GRT_CODTRA" />
            </key-property>
        </composite-id>
    </class>

</hibernate-mapping>

