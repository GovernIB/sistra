<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<class  name="es.caib.sistra.model.GestorFormulario" table="STR_GESFRM">
	<cache usage="read-write"/>
    <id name="identificador" column="GSF_IDENT">
        <generator class="assigned" />
    </id>
    <property name="descripcion"  column="GSF_DESC"  not-null="true"/>
    <property name="urlGestor"  column="GSF_URL"  not-null="true"/>
    <property name="urlTramitacionFormulario"  column="GSF_URLTRA"  not-null="true"/>
    <property name="urlRedireccionFormulario"  column="GSF_URLRED"  not-null="true"/>
</class>
</hibernate-mapping>
