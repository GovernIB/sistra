<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE form-validation PUBLIC
          "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.0//EN"
          "http://jakarta.apache.org/commons/dtds/validator_1_0.dtd">

<form-validation>

    <formset>
		<form name="tramiteForm">
			<field property="values.identificador" depends="required" page="1">
                <arg0 key="tramite.identificador" />
            </field>
   			<field property="values.descripcion" depends="required" page="1">
                <arg0 key="tramite.descripcion" />
            </field>
		</form>
		<form name="gestorBandejaForm">
			<field property="values.seyconID" depends="required" page="1">
                <arg0 key="gestorBandeja.seyconID" />
            </field>
   			<field property="values.email" depends="required" page="1">
                <arg0 key="gestorBandeja.email" />
            </field>
		</form>		
    </formset>

</form-validation>