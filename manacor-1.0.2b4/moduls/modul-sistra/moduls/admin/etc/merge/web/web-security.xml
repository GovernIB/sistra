<security-constraint>
    <web-resource-collection>
        <web-resource-name>sistraadmin</web-resource-name>
        <url-pattern>/*</url-pattern>
    </web-resource-collection>
    <auth-constraint>
        <role-name>@role.sistra@</role-name>
        <role-name>@role.auditor@</role-name>
    </auth-constraint>
</security-constraint>

@web.login-config@

<security-role>
    <role-name>@role.sistra@</role-name>
</security-role>
<security-role>
	<role-name>@role.auditor@</role-name>
</security-role>	
	    
